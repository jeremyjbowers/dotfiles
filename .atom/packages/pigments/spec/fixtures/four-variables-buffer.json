{
  "id": #{id},
  "path": "#{root}/four-variables.styl",
  "variableMarkers": [
    {
      "markerId": #{[variableMarkers]},
      "bufferRange": [[0,0],[0,17]],
      "variable": "base-color"
    },
    {
      "markerId": #{[variableMarkers]},
      "bufferRange": [[2,0],[2,45]],
      "variable": "other-color"
    },
    {
      "markerId": #{[variableMarkers]},
      "bufferRange": [[4,0],[4,27]],
      "variable": "other_color"
    },
    {
      "markerId": #{[variableMarkers]},
      "bufferRange": [[6,0],[6,39]],
      "variable": "$other-color"
    }
  ],
  "colorMarkers": [
    {
      "markerId": #{[colorMarkers]},
      "bufferRange": [[0,13],[0,17]],
      "color": [255,255,255,1],
      "text": "#fff",
      "variables": []
    },
    {
      "markerId": #{[colorMarkers]},
      "bufferRange": [[4,14],[4,27]],
      "color": [0,0,0,1],
      "text": "rgba(0,0,0,1)",
      "variables": []
    },
    {
      "markerId": #{[colorMarkers]},
      "bufferRange": [[6,15],[6,39]],
      "color": [255,0,0,0.5],
      "text": "transparentize(red, 0.5)",
      "variables": []
    },
    {
      "markerId": #{[colorMarkers]},
      "bufferRange": [[2,14],[2,45]],
      "color": [255,255,255,0.5],
      "text": "transparentize(base-color, 50%)",
      "variables": ["base-color"]
    }
  ]
}
