{
  "deserializer": "ColorProject",
  "timestamp": "#{timestamp}",
  "version": "#{version}",
  "ignoredNames": [
    "vendor/*"
  ],
  "paths": [
    "#{root}/styles/buttons.styl",
    "#{root}/styles/variables.styl"
  ],
  "variables": [
    {
      "name": "colors.red",
      "value": "#BF616A",
      "range": [
        13,
        25
      ],
      "path": "#{root}/styles/variables.styl"
    }
  ]
}
